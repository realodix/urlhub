APP_NAME=UrlHub
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

UH_PUBLIC_SITE=true
UH_REGISTRATION=true
UH_HASH_LENGTH=6
UH_HASH_CHAR=abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890
UH_REDIRECT_CACHE_MAX_AGE=30
